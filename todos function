// Code goes here
var todos= ['item 1','item 2','item 3']

//display todos
function displayTodos(){
  console.log('my todos:',todos);
}

//add todos
function addTodos (newtodo){
  todos.push(newtodo);
  displayTodos();
}
addTodos('test')

//change todos
function changeTodos (order,newtodo){
  todos[order]=newtodo;
  displayTodos();
}
changeTodos(0,'changed panda')

// remove todos
function remTodo (order){
  todos.splice(order,1);
  displayTodos();
}
